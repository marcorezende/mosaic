{
  "$schema": "https://idl.uw.edu/mosaic/schema/v0.12.2.json",
  "data": {
    "aapl": {
      "type": "parquet",
      "file": "data/stocks.parquet",
      "where": "Symbol = 'AAPL'"
    }
  },
  "plot": [
    {
      "mark": "lineY",
      "data": {
        "from": "aapl"
      },
      "x": "Date",
      "y": "Close"
    }
  ],
  "width": 680,
  "height": 200
}